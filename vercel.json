{
  "name": "nowshowing-app",
  "builds": [
    {
      "src": "api/check-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/fetch-news.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/omdb-proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    },
    {
      "src": "service-worker.js",
      "use": "@vercel/static"
    },
    {
      "src": "manifest.webmanifest",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com https://fonts.googleapis.com; img-src 'self' https: data: blob: *; font-src 'self' https://fonts.gstatic.com https://cdnjs.cloudflare.com data:; connect-src 'self' https://www.omdbapi.com https://gnews.io https:; frame-src https://vidsrc.to https://vidsrc.xyz https://vidsrc.in https://vidcloud.stream https://fsapi.xyz https://superembed.stream https://2embed.cc https://moviesapi.club https://fmovies.to https://lookmovie.io https://curtstream.com https://autoembed.cc https://multiembed.mov; frame-ancestors 'self';",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "Permissions-Policy": "geolocation=(), microphone=(), camera=()"
      },
      "continue": true
    },
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/service-worker.js",
      "dest": "/service-worker.js"
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "/manifest.webmanifest"
    },
    {
      "src": "/api/check-video",
      "dest": "/api/check-video.js"
    },
    {
      "src": "/api/fetch-news",
      "dest": "/api/fetch-news.js"
    },
    {
      "src": "/api/omdb-proxy",
      "dest": "/api/omdb-proxy.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}