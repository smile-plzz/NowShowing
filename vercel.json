{
  "name": "nowshowing-app",
  "builds": [
    {
      "src": "api/check-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/check-video.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}